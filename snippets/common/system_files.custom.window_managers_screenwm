# screenwm
[type=file uid=0 gid=0 perms=0755] bin/screenwm={
#/bin/sh
SCREENWM_DIR=/system/opt/screenwm
cd $SCREENWM_DIR
./screenwm
}
[type=file uid=0 gid=0 perms=0755] opt/screenwm/screenwm=bin/screenwm
[type=file uid=0 gid=0 perms=0755] opt/screenwm/theme.so=lib/libphoton.so.1

[type=file uid=0 gid=0 perms=0644] opt/screenwm/screenwm.ini={
[General]
background=/system/etc/images/background_1080p.png
taskbar=bottom
screenIdle=20
}

[type=file uid=0 gid=0 perms=0644] opt/screenwm/terminal.desktop={
[Desktop Entry]
Name=Terminal
Exec=/system/bin/st /system/etc/st_boot.sh
Icon=/system/share/icons/terminal_black.png
Categories=Utilities
}

